<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Platforms>AnyCPU;x64</Platforms>
    <Version>0.1.0.0</Version>
    <Authors>LC</Authors>
    <Company>LC</Company>
    <Description>在二进制数据与 ATGC 文本间转换。</Description>
    <Copyright>Copyright © 2021 LC. All rights reserved.</Copyright>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/lc6464/ATGCCoder</PackageProjectUrl>
    <PackageIcon>LCx256.png</PackageIcon>
    <RepositoryUrl>https://github.com/lc6464/ATGCCoder.git</RepositoryUrl>
    <RepositoryType>Public</RepositoryType>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <SignAssembly>false</SignAssembly>
    <AssemblyOriginatorKeyFile />
    <ApplicationIcon>LC.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="LCx256.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
